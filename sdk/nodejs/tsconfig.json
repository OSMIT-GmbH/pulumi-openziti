{
    "compilerOptions": {
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,
        "experimentalDecorators": true,
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    },
    "files": [
        "config/index.ts",
        "config/vars.ts",
        "configObj.ts",
        "edgeRouter.ts",
        "edgeRouterPolicy.ts",
        "enrolledIdentity.ts",
        "identity.ts",
        "index.ts",
        "provider.ts",
        "service.ts",
        "serviceEdgeRouterPolicy.ts",
        "servicePolicy.ts",
        "types/index.ts",
        "types/input.ts",
        "types/output.ts",
        "utilities.ts"
    ]
}
